<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/map_filter_v2_linear_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".viewmodel.MapFilterActivityV2">

    <TextView
        android:id="@+id/categoryFiltersTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:singleLine="false"
        android:text="@string/select_the_categories_of_meetups_that_you_would_like_to_see_on_the_map"
        android:textSize="20sp" />
</LinearLayout>